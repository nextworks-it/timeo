                            "enum": [
                                "LOCKOUT",
                                "NORMAL",
                                "MANUAL",
                                "FORCED",
                                "WAIT_TO_REVERT",
                                "SIGNAL_DEGRADE",
                                "SIGNAL_FAIL"
                            ],
                            "type": "string",
                            "description": "The reason for the current switch selection."
                        },
                        "switch-direction": {
                            "enum": [
                                "BIDIRECTIONAL",
                                "INPUT",
                                "OUTPUT",
                                "UNIDENTIFIED_OR_UNKNOWN"
                            ],
                            "type": "string",
                            "description": "Indicates whether the switch selects from ingress to the FC or to egress of the FC, or both."
